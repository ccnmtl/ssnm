<?xml version="1.0"?>
<html metal:use-macro="sitemacros/macros/ecomap">
<body metal:fill-slot="body">
<div id="header"><img src="/images/logo.png" /></div>
<br/>

<form tal:attributes="action postTo" method="post">
<fieldset>
<legend>Create Social Support Network Map</legend>
<label>Subject Name: <input type="text" name="name" size="60" /></label><form:iferror name="name"> <span class="errormessage"><form:error name="name"/></span></form:iferror><br />
<label>Map Description: <input type="text" name="description" size="60" /></label><form:iferror name="description"> <span class="errormessage"><form:error name="description"/></span></form:iferror><br />

<label>Course ID: (debug)<input type="text" name="course" size="6" /></label><form:iferror name="course"> <span class="errormessage"><form:error name="course"/></span></form:iferror><br />


<input type="submit" value="Create"/>
</fieldset>
</form>
</body>

</html>
