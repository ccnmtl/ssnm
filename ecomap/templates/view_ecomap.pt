<?xml version="1.0"?>

<html metal:use-macro="sitemacros/macros/ecomap">

	<div metal:fill-slot="content" id="content">
	
		<div id="toolbar">
			<div id="standard">
				<a href="#">
					About
				</a>
				<a href="#">
					Help
				</a>
				<a href="#">
					Contact
				</a>
			</div>
			
			<div id="log">
				Logged in as 
				<span tal:content="loginName" />
				| 
				<a href="/logout">
					Logout
				</a>
			</div>
			
		</div>
		
		<div id="breadcrumbbar">
		?
		</div>
		
		<metal:block define-macro="message">
			<div tal:condition="message" tal:content="message" class="message" />
		</metal:block>
		
		<div id="flash">
		
			<object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" codebase="http://fpdownload.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,0,0" width="80%" height="80%" id="ecomap" align="middle">
				<param name="allowScriptAccess" value="sameDomain" />
				<param name="movie" value="/flash/ecomap.swf" />
				<param name="loop" value="false" />
				<param name="menu" value="false" />
				<param name="quality" value="high" />
				<param name="scale" value="noscale" />
				<param name="salign" value="lt" />
				<param name="bgcolor" value="#ffffff" />
				<param name="flashVars" tal:attributes="value string:HTMLticket=${ticket}&amp;HTMLid=${id}&amp;myName=${myName}&amp;server=${server}&amp;returnPath=${returnPath}" />
				<embed src="/flash/ecomap.swf" loop="false" menu="false" quality="high" scale="noscale" salign="lt" bgcolor="#ffffff" width="80%" height="80%" name="iat" align="middle" allowscriptaccess="sameDomain" type="application/x-shockwave-flash" pluginspage="http://www.macromedia.com/go/getflashplayer" tal:attributes="flashvars string:HTMLticket=${ticket}&amp;HTMLid=${id}&amp;myName=${myName}&amp;server=${server}&amp;returnPath=${returnPath}" />
			</object>
			
		</div>
		
	</div>
	
</html>
