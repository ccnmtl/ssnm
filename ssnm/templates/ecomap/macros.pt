<?xml version="1.0"?>

<html metal:define-macro="ecomap">

	<head>
	
		<title metal:define-slot="page_title">
			Social Support Network Map
		</title>
		
		<link rel="stylesheet" href="/css/ecomap.css" type="text/css" />
		
		<link rel="icon" href="http://ccnmtl.columbia.edu/web/images/favicon.ico" type="image/x-icon" />
		
		<metal:block define-slot="js_slot"></metal:block>
		
		<metal:block define-slot="css_slot"></metal:block>
		
		<!--[if IE]>
		<style type="text/css">

		table.infotable
		{
			margin-bottom: 20px;
			width: 96%;
		}
		
		</style>
		<![endif]-->	
		
	</head>
	
	<body metal:define-slot="body">
	
		<h1 metal:define-slot="header">
		<a href="/course/" title="">Social Support Network Map</a>
		</h1>
		
		<div id="ccnmtl">
		<a href="http://ccnmtl.columbia.edu/" title="Go to the CCNMTL web site." target="_blank">CCNMTL</a>
		</div>

		<div id="toolbar">
		
			<div id="standard">
				<a href="/about">About</a>
				<a href="/help">Help</a>
				<tal:block condition="login_name">
					<a href="/contact">Feedback</a>
				</tal:block>
			</div>
			
			<div id="log">
				<tal:block condition="login_name">
				Logged in as <span tal:content="login_name" /> | <a href="/logout">Logout</a>
				</tal:block>
				<tal:block condition="not:login_name">
				<a href="/login">Log In</a>
				</tal:block>
			</div>
			
		</div>
		
		<div id="content" metal:define-slot="content">
			This is where the page content goes
		</div>

		
	</body>
	
</html>
